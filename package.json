{
  "name": "patient-browser",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=7.9.0"
  },
  "scripts": {
    "test": "./node_modules/karma/bin/karma start ./karma.conf.js --no-auto-watch --single-run",
    "test:watch": "./node_modules/karma/bin/karma start ./karma.conf.js",
    "clean": "cross-os do-clean",
    "start": "http-server build",
    "postinstall": "npm run build",
    "build": "if-env NODE_ENV=production && npm run build:prod || npm run build:dev",
    "build:dev": "npm run clean && webpack --hide-modules && webpack-dev-server",
    "build:prod": "npm run clean && webpack",
    "test:e2e": "node ./selenium-download.js && ./node_modules/nightwatch/bin/nightwatch"
  },
  "cross-os": {
    "do-clean": {
      "darwin": "rm -f ./build/js/*.* && rm -f ./build/index.html",
      "win32": "del /Q build\\js\\*.* && del /Q build\\index.html",
      "linux": "rm -f ./build/js/*.* && rm -f ./build/index.html"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chb/patient-browser.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chb/patient-browser/issues"
  },
  "homepage": "https://github.com/chb/patient-browser#readme",
  "dependencies": {
    "autoprefixer": "^10.0.0",
    "babel": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "colors": "^1.2.1",
    "commander": "^9.0.0",
    "cross-os": "^1.3.0",
    "css-loader": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "history": "^5.0.0",
    "html-webpack-plugin": "^5.0.0",
    "http-server": "^14.0.0",
    "if-env": "^1.0.0",
    "jquery": "^3.2.1",
    "json5": "^2.0.0",
    "less": "^4.0.0",
    "less-loader": "^11.0.0",
    "mixin-deep": "^2.0.0",
    "moment": "^2.18.1",
    "postcss-loader": "^7.0.0",
    "prop-types": "^15.5.10",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.0",
    "react-router": "^6.0.0",
    "react-router-dom": "^6.0.0",
    "react-with-addons": "0.0.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.0.1",
    "redux-thunk": "^2.2.0",
    "redux-thunk-actions": "^1.1.6",
    "request": "^2.85.0",
    "style-loader": "^3.0.0",
    "webpack": "^5.0.0",
    "webpack-bundle-analyzer": "^4.0.0"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions",
    "ie 10"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.0.0",
    "chai": "^4.0.0",
    "json5-loader": "^4.0.0",
    "karma": "^6.0.0",
    "karma-chai": "^0.1.0",
    "karma-firefox-launcher": "^2.0.0",
    "karma-mocha": "^2.0.0",
